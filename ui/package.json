{
  "name": "standard-vue",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node build/dev-server.js",
    "build": "API_ENV=prod node build/build.js",
    "browsers": "browserslist"
  },
  "standard": {
    "globals": [
      "axios",
      "_util",
      "_global",
      "_displayError",
      "_showErr",
      "_showSuccess",
      "__API_ENV__",
      "_showConfirm",
      "_db"
    ]
  },
  "dependencies": {
    "@uppy/core": "1.7.1",
    "@uppy/dashboard": "1.5.2",
    "@uppy/xhr-upload": "1.4.2",
    "axios": "0.19.2",
    "date-fns": "2.9.0",
    "husky": "4.2.1",
    "qrcode": "1.4.4",
    "qs": "6.9.1",
    "tailwindcss": "1.1.4",
    "vue": "2.6.11",
    "vue-js-modal": "1.3.31",
    "vue-notification": "1.3.20",
    "vue-router": "3.1.5",
    "vuex": "3.1.2"
  },
  "devDependencies": {
    "@babel/core": "7.8.4",
    "@babel/polyfill": "7.8.3",
    "@babel/preset-env": "7.8.4",
    "@cjting/standard": "1.1.0",
    "autoprefixer": "9.7.4",
    "babel-loader": "8.0.6",
    "chalk": "3.0.0",
    "connect-history-api-fallback": "1.6.0",
    "copy-webpack-plugin": "5.1.1",
    "css-loader": "3.4.2",
    "express": "4.17.1",
    "file-loader": "5.0.2",
    "friendly-errors-webpack-plugin": "1.7.0",
    "html-webpack-plugin": "3.2.0",
    "http-proxy-middleware": "0.20.0",
    "mini-css-extract-plugin": "0.9.0",
    "opn": "6.0.0",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "ora": "4.0.3",
    "postcss-loader": "3.0.0",
    "pug": "2.0.4",
    "pug-plain-loader": "1.0.0",
    "rimraf": "3.0.1",
    "snazzy": "8.0.0",
    "stylus": "0.54.7",
    "stylus-loader": "3.0.2",
    "terser-webpack-plugin": "2.3.4",
    "url-loader": "3.0.0",
    "vue-loader": "15.8.3",
    "vue-style-loader": "4.1.2",
    "vue-template-compiler": "2.6.11",
    "webpack": "4.41.5",
    "webpack-bundle-analyzer": "3.6.0",
    "webpack-dev-middleware": "3.7.2",
    "webpack-hot-middleware": "2.25.0",
    "webpack-merge": "4.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "standard 'src/**/*.{js,vue}' | snazzy"
    }
  }
}
